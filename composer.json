{
  "name": "blazervel/feature",
  "description": "Zero-configuration Laravel development workflow",
  "license": "MIT",
  "authors": [
      {
          "name": "Joshua Anderton",
          "email": "hey@joshuaanderton.ca"
      }
  ],
  "require": {
      "php": "^8.0",
      "illuminate/support": "^8.67|^9.0",
      "lorisleiva/laravel-actions": "^2.4",
      "phpstan/phpstan": "^1.7"
  },
  "require-dev": {
      "phpunit/phpunit": "^9.3"
  },
  "autoload": {
      "psr-4": {
          "Blazervel\\Feature\\": "src"
      }
  },
  "autoload-dev": {
      "psr-4": {
          "Blazervel\\Feature\\Tests\\": "tests"
      }
  },
  "config": {
      "sort-packages": true
  },
  "extra": {
      "laravel": {
          "providers": [
              "Blazervel\\Feature\\Providers\\BlazervelFeatureServiceProvider"
          ]
      }
  }
}
